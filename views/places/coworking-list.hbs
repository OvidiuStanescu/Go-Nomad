{{#each coworkRes}}
<div class="places-display">
    <div class="container">
        <h5 class="card-title top-margin">{{types.[0]}}</h5>
        <h1 style="font-weight:bold" class="card-subtitle mb-2">{{name}}</h1>
        <p class="card-text">Rating: {{rating}}</p>
        <a href="/places/details/coworkings/{{name}}" class="card-link">Coworking Details</a>
        <hr>
    </div>
</div>
{{/each}}